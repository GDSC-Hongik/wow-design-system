{
  "name": "wowds-ui",
  "version": "0.0.12",
  "description": "",
  "author": "gdsc-hongik",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GDSC-Hongik/wow-design-system.git",
    "directory": "packages/wow-ui"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "module": "./dist/components/index.js",
  "type": "module",
  "exports": {
    "./styles.css": "./dist/styles.css",
    "./TextField": {
      "types": "./dist/components/TextField/index.d.ts",
      "require": "./dist/TextField.cjs",
      "import": "./dist/TextField.js"
    },
    "./Switch": {
      "types": "./dist/components/Switch/index.d.ts",
      "require": "./dist/Switch.cjs",
      "import": "./dist/Switch.js"
    },
    "./RadioButton": {
      "types": "./dist/components/RadioGroup/RadioButton.d.ts",
      "require": "./dist/RadioButton.cjs",
      "import": "./dist/RadioButton.js"
    },
    "./RadioGroup": {
      "types": "./dist/components/RadioGroup/RadioGroup.d.ts",
      "require": "./dist/RadioGroup.cjs",
      "import": "./dist/RadioGroup.js"
    },
    "./ProgressBar": {
      "types": "./dist/components/ProgressBar/index.d.ts",
      "require": "./dist/ProgressBar.cjs",
      "import": "./dist/ProgressBar.js"
    },
    "./MultiGroup": {
      "types": "./dist/components/MultiGroup/index.d.ts",
      "require": "./dist/MultiGroup.cjs",
      "import": "./dist/MultiGroup.js"
    },
    "./Chip": {
      "types": "./dist/components/Chip/index.d.ts",
      "require": "./dist/Chip.cjs",
      "import": "./dist/Chip.js"
    },
    "./Checkbox": {
      "types": "./dist/components/Checkbox/index.d.ts",
      "require": "./dist/Checkbox.cjs",
      "import": "./dist/Checkbox.js"
    },
    "./Button": {
      "types": "./dist/components/Button/index.d.ts",
      "require": "./dist/Button.cjs",
      "import": "./dist/Button.js"
    },
    "./Box": {
      "types": "./dist/components/Box/index.d.ts",
      "require": "./dist/Box.cjs",
      "import": "./dist/Box.js"
    }
  },
  "keywords": [],
  "license": "ISC",
  "scripts": {
    "build": "pnpm generate:build-config && rm -rf dist && rollup -c --bundleConfigAsCjs && tsc-alias && panda cssgen --outfile dist/styles.css",
    "lint": "eslint . --max-warnings 0",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:build-config": "tsx ../scripts/generateBuildConfig.ts && prettier --write ./rollup.config.js ./package.json",
    "test-storybook": "test-storybook --browsers firefox chromium webkit",
    "test": "jest",
    "create:story-file": "plop --plopfile ../codegen/plopfile.js"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@storybook/addon-a11y": "^8.1.9",
    "@storybook/addon-essentials": "^8.1.9",
    "@storybook/addon-interactions": "^8.1.9",
    "@storybook/addon-links": "^8.1.9",
    "@storybook/addon-onboarding": "^8.1.9",
    "@storybook/blocks": "^8.1.9",
    "@storybook/nextjs": "^8.1.9",
    "@storybook/react": "^8.1.9",
    "@storybook/test": "^8.1.9",
    "@storybook/test-runner": "^0.18.2",
    "@storybook/testing-library": "^0.2.2",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "axe-playwright": "^2.0.1",
    "chromatic": "^11.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^0.8.0",
    "plop": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "storybook": "^8.1.9",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "wowds-icons": "workspace:^"
  },
  "peerDependencies": {
    "next": "^14.1.1",
    "react": "^18.2.0"
  },
  "bugs": {
    "url": "https://github.com/GDSC-Hongik/wow-design-system/issues"
  },
  "homepage": "https://github.com/GDSC-Hongik/wow-design-system#readme"
}
